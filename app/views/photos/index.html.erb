<div class="row">
  <div class="col-md-12">
    <% if current_user %>
      <%#= link_to "New Photo", new_photo_path, :class => "btn btn-primary pull-right", "data-toggle" => "modal", "data-target" => "#panel-new" %>
      <%= button_tag "New Photo", :class => "btn btn-primary pull-right", "data-toggle" => "modal", "data-target" => "#panel-new" %>
    <% end %>
  </div>
</div>

<%= render :partial => "panel_new", :locals => {:photo => @photo}, :layout => false %>

<div class="row photo-shower">
  <div class="col-md-12 showroom">

    <% @photos.each do |photo| %>

      <%= render :partial => "onephoto", :locals => {:photo => photo} %>

    <% end %>
  </div>
</div>


